{
  "name": "tailwind-nextjs-with-prismic",
  "version": "0.0.1",
  "description": "An awesome template for a website made with Prismic (slicemachines), NextJS and Tailwind",
  "author": "George Gabriel Luta",
  "license": "MIT",
  "main": "pages/_app.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "prepublishOnly": "node ./node_modules/sm-commons/scripts/bundle",
    "postbuild": "next-sitemap",
    "sm": "prismic sm --develop",
    "sm:create": "prismic sm --create-slice",
    "slicemachine": "start-slicemachine --port 9999",
    "format": "prettier --write \"{.slicemachine,.storybook,components,context,customtypes,docs,pages,slices,styles,utils}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}\"",
    "format:check": "prettier --check '{.slicemachine,.storybook,components,context,,customtypes,docs,pages,slices,styles,utils}/**/*.{js,jsx,ts,tsx,css,scss,json,yaml,md}'",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "kill": "kill $(lsof -ti:3000,8888,9999)"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@headlessui/react": "^1.5.0",
    "@prismicio/client": "^6.3.0",
    "@prismicio/helpers": "^2.1.1",
    "@prismicio/react": "^2.0.6",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.2",
    "next": "^12.1.0",
    "next-sitemap": "^2.5.1",
    "next-slicezone": "^0.2.1",
    "next-transpile-modules": "^9.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-react-require": "^3.1.3",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.5.1",
    "prismic-cli": "^4.2.2",
    "shx": "^0.3.4",
    "slice-machine-ui": "^0.2.1",
    "sm-commons": "^0.1.1",
    "tailwindcss": "^3.0.23"
  },
  "resolutions": {
    "eslint-plugin-react": "7.28.0"
  }
}
